{
	"name": "RemoveRedlinks",
	"author": [
		"Chad Horohoe",
		"Malte Ahrholdt",
		"Dror S. [FFS] ([http://www.kolzchut.org.il Kol-Zchut]"
	],
	"version": "2.1.0",
	"url": "https://www.mediawiki.org/wiki/Extension:RemoveRedlinks",
	"descriptionmsg": "RemoveRedlinks-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "parser",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\RemoveRedlinks\\": "includes/"
	},
